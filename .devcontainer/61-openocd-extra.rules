# Copy this file to /etc/udev/rules.d/
# If rules fail to reload automatically, you can refresh udev rules
# with the command "udevadm control --reload"

ACTION!="add|change", GOTO="openocd_extra_end"

SUBSYSTEM=="gpio", MODE="0666", GROUP="plugdev", TAG+="uaccess"

SUBSYSTEM!="usb|tty|hidraw", GOTO="openocd_extra_rules_end"

# ESP32 DEVKIT v1
ATTRS{idVendor}=="1a86", ATTRS{idProduct}=="55d4", MODE="0666", GROUP="plugdev", TAG+="uaccess"
ATTRS{idVendor}=="1a86", ATTRS{idProduct}=="55d5", MODE="0666", GROUP="plugdev", TAG+="uaccess"

LABEL="openocd_extra_rules_end"